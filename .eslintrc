{
  "extends": [ "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "global-require": 0,
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "import/no-extraneous-dependencies": 0,
    "react/jsx-closing-bracket-location": [0, "tag-aligned"]
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
